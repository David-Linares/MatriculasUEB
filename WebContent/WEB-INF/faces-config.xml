<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <converter>
  <converter-id>carreraConverter</converter-id>
  <converter-class>co.uel.matriculas.converter.CarreraConverter</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>estudianteBean</managed-bean-name>
  <managed-bean-class>co.ueb.matriculas.beans.EstudianteBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>nombreEstudiante</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>facultadBean</managed-bean-name>
  <managed-bean-class>co.ueb.matriculas.beans.FacultadBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>listadoFacultades</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>nombreFacultad</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>facultadAux</property-name>
   <property-class>co.ueb.matriculas.model.Facultad</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>loginBean</managed-bean-name>
  <managed-bean-class>co.ueb.matriculas.beans.LoginBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>usuario</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>contrasena</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>carreraBean</managed-bean-name>
  <managed-bean-class>co.ueb.matriculas.beans.CarreraBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>listadoCarreras</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>nombreCarrera</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>carreraAux</property-name>
   <property-class>co.ueb.matriculas.model.Carrera</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>matriculaBean</managed-bean-name>
  <managed-bean-class>co.ueb.matriculas.beans.MatriculaBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>materiaBean</managed-bean-name>
  <managed-bean-class>co.ueb.matriculas.beans.MateriaBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>paginaEstudiante</from-outcome>
   <to-view-id>Modulos/estudiante.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>paginaCarrera</from-outcome>
   <to-view-id>Modulos/carrera.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>paginaFacultad</from-outcome>
   <to-view-id>Modulos/facultad.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>Modulos/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>paginaMatricula</from-outcome>
   <to-view-id>Modulos/matricula.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>paginaMateria</from-outcome>
   <to-view-id>Modulos/materia.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>error</from-outcome>
   <to-view-id>/error.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
