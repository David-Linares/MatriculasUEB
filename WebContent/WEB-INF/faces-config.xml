<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>estudianteBean</managed-bean-name>
  <managed-bean-class>co.ueb.matriculas.beans.EstudianteBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>nombreEstudiante</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>facultadBean</managed-bean-name>
  <managed-bean-class>co.ueb.matriculas.beans.FacultadBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>nombreFacultad</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>paginaEstudiante</from-outcome>
   <to-view-id>Modulos/Estudiante/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>paginaCarreras</from-outcome>
   <to-view-id>Modulos/Carreras/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>paginaFacultad</from-outcome>
   <to-view-id>Modulos/Facultad/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>Modulos/Login/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>paginaMatricula</from-outcome>
   <to-view-id>Modulos/Matricula/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>error</from-outcome>
   <to-view-id>/error.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
