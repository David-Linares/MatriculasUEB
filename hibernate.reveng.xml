	<table-filter match-name="MATERIA_MATRICULA"></table-filter>
	<table-filter match-name="MATRICULA"></table-filter>
	<table-filter match-name="PERFIL"></table-filter>
	<table-filter match-name="PERFIL"></table-filter>
	<table-filter match-name="PERSONA"></table-filter>
	<table-filter match-name="TELEFONOS"></table-filter>
	<table name="MATERIA_MATRICULA">
		<primary-key></primary-key>
	</table>
	<table name="MATERIA"></table>
	<table name="MATRICULA"></table>
	<table name="PERFIL"></table>
	<table name="CARRERA_MATERIA"></table>
	<table name="TELEFONOS"></table>
	<table name="FACULTAD"></table>
